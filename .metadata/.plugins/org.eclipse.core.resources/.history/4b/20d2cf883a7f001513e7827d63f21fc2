package agent_trade.model;

import java.io.Serializable;


public class Agente implements Serializable{
	private String idAgente;
	private String nome;
	private String cognome;
	//public Vector<Preventivo> _crea = new Vector<Preventivo>();
	
	private static Agente instance;

	
	public Agente(){		
	}
	
	
	public Agente(String cognome, String nome){
		super();
		this.cognome=cognome;
		this.nome=nome;			
	}
	
	public static Agente getInstance(){

		return ((instance == null) ? instance = new Agente() : instance);	
	}
	
	
	
	public String getNome(){
		return this.nome;
	}

	
	public String getCognome() {
		return cognome;
	}

	
	public void setCognome(String cognome) {
		this.cognome = cognome;
	}


	public void setNome(String nome) {
		this.nome = nome;
	} 
}

