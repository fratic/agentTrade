package agent_trade.persistentTemp;

import java.io.FileOutputStream;
import java.io.ObjectOutputStream;

public class Dao {

	//Salva i dati della nel file specificato
	public void salva(String prova){
	ObjectOutputStream oss;
	try{
	oss = new ObjectOutputStream(new FileOutputStream(prova));
	oss.writeObject(this);
	oss.close();
	}
	catch(Exception e){
	//messaggio di errore
	}
	} 

/*
