<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="agent_trade.model.M_Catalogo" table="Catalogo" lazy="false">
		<id name="idCatalogo" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="rifAzienda" cascade="lock" column="AziendaID" class="agent_trade.model.M_Azienda" not-null="true" lazy="proxy">
		</many-to-one>
		<list name="prodotti" lazy="true" cascade="save-update,lock" inverse="false">
			<key column="CatalogoID" not-null="false"/>
			<index column="CatalogoIndex" type="integer"/>
			<one-to-many class="agent_trade.model.M_Prodotto"/>
		</list>
	</class>
</hibernate-mapping>
