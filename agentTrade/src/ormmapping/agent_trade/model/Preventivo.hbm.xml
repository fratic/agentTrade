<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="agent_trade.model.M_Preventivo" table="Preventivo" lazy="false">
		<id name="idPreventivo" column="ID" type="string">
			<generator class="assigned">
			</generator>
		</id>
		<many-to-one name="rif_Cliente" cascade="lock" column="ClienteID" class="agent_trade.model.M_Cliente" not-null="true" lazy="proxy">
		</many-to-one>
		<many-to-one name="rif_Agente" cascade="lock" column="AgenteID" class="agent_trade.model.M_Agente" not-null="true" lazy="proxy">
		</many-to-one>
		<property name="data" column="Data" type="date" not-null="false" lazy="false"/>
		
		<list name="elencoITem" lazy="true" cascade="all-delete-orphan" inverse="false" table="Preventivo_Item">
			<key column="PreventivoID" not-null="false"/>
			<index column="PreventivoIndex" type="integer"/>



	
			<one-to-many class="agent_trade.model.M_Preventivo_Item"/>	

		</list>
				
	</class>
</hibernate-mapping>





