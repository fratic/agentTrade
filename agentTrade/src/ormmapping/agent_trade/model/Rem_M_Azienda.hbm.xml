<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="agent_trade.model.M_Azienda" table="Azienda" lazy="false">
		<id name="IdAzienda" column="IdAzienda" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="ragioneSociale" column="RagioneSociale" type="string" length="255" not-null="false" lazy="false"/>
		<property name="codiceFiscale" column="CodiceFiscale" type="string" length="255" not-null="false" lazy="false"/>
		<property name="partitaIva" column="PartitaIva" type="string" length="255" not-null="false" lazy="false"/>
		<property name="citta" column="Citta" type="string" length="255" not-null="false" lazy="false"/>
		<property name="cap" column="Cap" type="string" length="255" not-null="false" lazy="false"/>
		<property name="indirizzo" column="Indirizzo" type="string" length="255" not-null="false" lazy="false"/>
		<property name="email" column="Email" type="string" length="255" not-null="false" lazy="false"/>
		<property name="telefono" column="Telefono" type="string" length="255" not-null="false" lazy="false"/>
		<property name="fax" column="Fax" type="string" length="255" not-null="false" lazy="false"/>
		<property name="url" column="Url" type="string" length="255" not-null="false" lazy="false"/>
		<property name="tipoProdotto" column="tipoProdotto" type="string" length="255" lazy="false"/>
		<property name="versione" column="versione" type="integer" length="11" not-null="false" lazy="false"/>
		
	</class>
</hibernate-mapping>
